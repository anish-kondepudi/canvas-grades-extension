{
	"manifest_version": 2,
	"name": "Canvas Grades Extension",
	"version": "0.0.0.2",
	"description": "An Extension to view all your Grades on the Courses Pages!",
	"content_scripts":
	[{
      "matches": ["https://*/"],
      "js": ["EditDashboard.js"]
    }],
	"icons":
	{
		"128": "canvas_logo.png",
		"48": "canvas_logo.png",
		"16": "canvas_logo.png"
	},
	"browser_action":
	{
		"default_icon": "canvas_logo.png",
		"default_popup": "popup.html",
		"default_title": "Canvas Grades Extension"
	}
}
